<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
  
 <hibernate-mapping>  
  <class name="bdstudia.Bank" table="banki">  
    <id name="idbanku">  
     <generator class="increment"></generator>  
    </id>  
            
    <property name="nazwa"></property>  
    <property name="adres"></property>  
    <property name="miasto"></property>  
    <property name="kodpocztowy"></property>  
    <property name="kraj"></property>  
            
  </class>  
            
 </hibernate-mapping>  
